<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schoolday">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lms"
        android:label="School Day"
        android:roundIcon="@drawable/lms"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".WelcomeActivity"></activity>
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".Signup2Activity" />
        <activity android:name=".student.MainActivity" />
        <activity android:name=".student.NotesEditAddActivity" />
        <activity android:name=".student.OnlineFriendsActivity" />
        <activity android:name=".student.EditActivity" />
        <activity
            android:name=".SignupActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>